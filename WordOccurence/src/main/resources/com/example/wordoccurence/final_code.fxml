<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.wordoccurence.FinalCode"
            prefHeight="400.0" prefWidth="600.0">

</AnchorPane>
<BorderPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FinalCodeController">
<center>
    <ScrollPane fitToWidth="true">
        <content>
            <TextArea fx:id="textArea" editable="false" />
        </content>
    </ScrollPane>
</center>
<bottom>
    <Button fx:id="showOutputButton" text="Show Output" onAction="#showOutput" />
</bottom>
</BorderPane>